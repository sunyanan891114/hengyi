    "babel-loader": "^6.0.1",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
